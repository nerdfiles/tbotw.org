@linei:@template"1"/**
 * Theme Name: BOTWv1
 * Theme URI: http://thebridgeovertroubledwaters.com
 * Description: For AIR Houston
 * Version: 1.0
 * Author: Kevin Fargason (Dev), Aaron Alexander (Dev), Alan Valek (Des), Germain Preston (IA)
 * Author URI: http://nerdfiles.net
 * Tags: bridges
 * Template: hybrid
 */

/* ======= Typesetting ======= */

@import url('_css/typesetting.css');

/* ======= Dev ======= */

@import url('_css-dev/nerdfiles.css');

/* ======= Element Styles ======= */

/* GLOBAL */

h3 {
  font-family: "Merriweather", Georgia, "Times New Roman", Times, serif;
  font-size: 1.4em;
  color: #000;
  padding-top: 5px;
  margin-bottom: 0px;
  font-weight: normal; }

.pod h3 {
  color: #630077;
  padding-top: 0px;
  margin-bottom: 10px; }

p {
  font-size: 1em;
  line-height: 1.45;
  margin-bottom: 10px; }

a {
  font-family: sans-serif, Helvetica, Arial;
  color: #630077;
  text-decoration: underline;
  &:visited {
    color: #777; }
  &:hover {
    color: #c199c2; }
  &:active, &:focus {
    color: #000; } }

blockquote {
  font-size: 1.7em;
  color: #630077;
  padding-left: 90px;
  font-style: italic;
  font-family: "Merriweather", Georgia, "Times New Roman", Times, serif;
  background: url("_img-ui/mission_blockquote_bg.gif") no-repeat 0px 0px transparent;
  margin-bottom: 10px; }

/* MAIN BODY */

#escape-info {
  display: none; }

#page-background {
  height: 160px;
  width: 100%;
  background: url("_img-ui/header_left.gif") repeat-x #f5f5ed;
  position: absolute; }

/* HEADER */

#header {
  position: relative;
  background: url("_img-ui/header_center.jpg") no-repeat -225px 0px;
  height: 155px; }

#text-logo {
  display: block;
  line-height: 0;
  text-indent: -999px; }

#logo {
  height: 105px;
  position: relative;
  z-index: 100;
  span {
    display: none; } }

#logo-box {
  background: #FFF;
  width: 114px;
  height: 106px;
  text-align: center;
  padding-top: 8px;
  -moz-border-radius: 0px 0px 5px 5px;
  border-radius: 0px 0px 5px 5px;
  margin-left: 15px; }

#search {
  position: absolute;
  right: 0;
  top: 10px;
  vertical-align: top;
  overflow: visible;
  div {
    float: right;
    margin-left: 10px; } }

#search-text {
  background: url("_img-ui/search_bar.gif") no-repeat scroll 0 0 transparent;
  border: medium none;
  height: 20px;
  width: 140px;
  text-indent: 5px;
  padding-top: 3px\9;
  *padding-top: 3px;
  padding-left: 5px\9;
  *padding-left: 5px; }

#search-submit {
  background: none repeat scroll 0 0 transparent;
  border: medium none;
  color: #FFFFFF;
  cursor: pointer;
  height: 22px;
  position: absolute;
  right: 8px;
  text-indent: -9999px;
  top: 0;
  width: 25px;
  z-index: 2000; }

#escape {
  font-family: 'Quattrocento Sans', 'Helvetica Neue', 'Helvetica', Arial, 'Liberation Sans', FreeSans, sans-serif;
  font-size: 1.3em;
  background-color: #C199C2;
  -moz-border-radius: 4px;
  border-radius: 4px;
  color: #630077;
  text-transform: uppercase;
  font-weight: bold;
  padding: 1px 5px 0px;
  display: block;
  width: 70px;
  text-decoration: none;
  float: right;
  text-align: center;
  line-height: 1.3; }

#top-nav {
  text-align: right;
  font-weight: bold;
  text-shadow: 0.1em 0.1em 0.1em rgba(0, 0, 0, 0.1);
  ul {
    color: #FFF;
    float: right;
    margin-top: 50px;
    margin-bottom: 10px;
    li {
      float: left;
      margin-right: 5px;
      margin-left: 10px;
      list-style: none;
      a {
        color: #FFFFFF;
        font-family: "Merriweather", Georgia, "Times New Roman", Times, serif;
        font-size: 1.2em;
        letter-spacing: -0.03em;
        text-decoration: none;
        &:hover {
          text-decoration: underline; } } } } }

/* END HEADER */

/* HOME HERO */

#hero {
  height: 270px;
  width: 100%;
  background: url("_img/home_page_hero.jpg") no-repeat;
  position: relative;
  margin-bottom: 15px;
  -moz-box-shadow: 0 0 .5em rgba(50,50,50,.7); 
  -webkit-box-shadow: 0 0 .5em rgba(50,50,50,.7); 
  box-shadow: 0 0 .5em rgba(50,50,50,.7); }

#hotline {
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  border-radius: 5px;
  color: #FFF;
  background: url("_img-ui/telephone-icon.gif") no-repeat 7px 7px white;
  position: absolute;
  bottom: 12px;
  left: 14px;
  padding: 2px 10px 2px 20px;
  -moz-box-shadow: 0 0 .5em rgba(50,50,50,.7); 
  -webkit-box-shadow: 0 0 .5em rgba(50,50,50,.7); 
  box-shadow: 0 0 .5em rgba(50,50,50,.7); 
  
  span {
    display: inline-block;
    margin-left: 6px;
    font-family: 'Quattrocento Sans', 'Helvetica Neue', 'Helvetica', Arial, 'Liberation Sans', FreeSans, sans-serif;
    font-weight: bold;
    font-size: .9em;
    color: #380785;
    line-height: 1.2;
    &.english {
      text-transform: uppercase; width: 55px; }
    &.spanish {
      font-weight: normal; width: 50px; }
    &.phone {
      font-size: 2.1em;
      letter-spacing: -0.02em;
      margin-left: 0;
      vertical-align: bottom; } } }

.hero-message {
  position: absolute;
  bottom: 14px;
  right: 14px;
  height: 65px;
  width: 500px;
  padding: 10px; }

#hero-message-box {
  background: #000;
  filter: alpha(opacity = 60);
  -moz-opacity: 0.7;
  opacity: 0.7;
  -moz-border-radius: 5px;
  border-radius: 5px; }

#hero-message-text.hero-message {
  color: #FFF;
  line-height: 1.3;
  h1 { text-shadow: none; }
  a {
    color: #FFF;
    line-height: 1;
    &:hover {
      color: #c199c2; } } }

#hero h1 {
  font-size: 1.9em;
  font-family: "Merriweather", Georgia, "Times New Roman", Times, serif;
  font-weight: normal;
  margin-bottom: 7px; }

/* END HOME HERO */

/* MAIN CONTENT */

#main-content {
  position: relative;
  top: 0;
  min-height: 35em;
  margin-bottom: 20px;
  h2 {
    color: #630077;
    font-family: "Merriweather", Georgia,"Times New Roman",Times,serif;
    font-size: 1.4em;
    font-weight: normal;
    margin-bottom: 10px; } }

.no-style {
  list-style: none; }

body.page-template-template-landing-page #main-content {
  margin-top: -75px;
  margin-bottom: 20px; }

#main-content h1 { 
  
  text-shadow: 1px 1px 1px #999;
  
  a {
    font-size: 1.2em;
    color: #630077;
    font-family: "Merriweather", Georgia, "Times New Roman", Times, serif;
    text-decoration: none;
    font-weight: normal; } }

#events {
  margin-top: 10px;
  h2 {
    font-size: 1.9em;
    color: #630077;
    font-family: "Merriweather", Georgia, "Times New Roman", Times, serif;
    font-weight: normal;
    float: left;
    margin-bottom: 0px; }
  h3 {
    padding-right: 60px;
    margin-bottom: 15px; }
  .h2 {
    a {
      float: right;
      margin-top: 10px; }
    background: url("_img-ui/brush-line-rule.gif") bottom left repeat-x;
    height: 40px; }
  .event {
    background: url("_img-ui/brush-line-rule.gif") bottom left repeat-x;
    position: relative;
    padding-bottom: 10px;
    .date {
      font-family: 'Quattrocento Sans', 'Helvetica Neue', 'Helvetica', Arial, 'Liberation Sans', FreeSans, sans-serif;
      font-size: 1.3em;
      background-color: #630077;
      -moz-border-radius: 4px;
      border-radius: 4px;
      color: #FFF;
      font-weight: normal;
      padding: 1px 5px 0px;
      position: absolute;
      top: 5px;
      right: 0px; } } }

.blockquote {
  margin-top: 10px;
  background: url("_img-ui/blockquote_bg_main.gif") top left no-repeat;
  line-height: 1.3;
  .top {}
  .bottom {
    background: url("_img-ui/blockquote_bg_shadow.gif") bottom left no-repeat;
    padding: 0px 20px 20px 20px; } }

#call-to-action-pods {
  margin-top: 10px;
  .pod {} }

/* FOOTER */

#footer-container {
  position: relative;
  /*top:-45px;	*/ }

.lmf_generated_text {
  position: absolute;
  left: -9999px; }

/* COMMENTS */

.comments-header {
  margin: 50px 0 15px;
  border-top: 3px #ccc double; 
  border-bottom: 1px #ccc solid; }

ol.comment-list {

  list-style: none;
  margin: 0;
  
  .comment {
    margin: 0 0 15px;
    clear: left;
    border-bottom: 1px #CCC solid; }
    
  .odd, .even, .depth-1, .depth-2, .depth-3 {}
  
  .avatar {
    float: left;
    margin: 0 15px 15px 0; }
    
  .comment-meta, .comment-author, .published, .permalink, .comment-edit-link, .comment-reply-link, .comment-text {} 
  
}

.addthis_toolbox {
  padding: 7px 10px 5px;
  background: rgba(255, 255, 255, 0.7);
  display: inline-block;
  *zoom: 1;
  *display: inline;
  -webkit-border-radius: .5em;
  -moz-border-radius: .5em;
  border-radius: .5em; }

.addthis_button_facebook_like {
  height: 1em; }

.fixed-icons { width: 960px; margin: 0 auto; position: relative; }

body .buttonfixed { 
  right: 15px !important;
  top: 85px; 
  position: absolute; }
body.home .buttonfixed { right: 120px !important; }

body .buttonfixed:hover { top: 85px !important; }

body.error-404 #primary-sidebar { display: none; }

