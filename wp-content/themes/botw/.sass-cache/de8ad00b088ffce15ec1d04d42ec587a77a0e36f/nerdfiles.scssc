/* ======= Utility Classes ======= */

.hide {
  display: none; }

.block {
  display: block; }

/* ======= Page Hero ======= */

#page-hero {
  height: 150px;
  width: 100%;
  background: url("../_img/partners_hero.jpg") no-repeat;
  position: relative;
  margin-top: -75px;
  margin-bottom: 15px; }

body {
  &.page-97 #page-hero {
    background-image: url("../_img/support_donate_hero.jpg"); }
  &.page-69 #page-hero {
    background-image: url("../_img/partners_hero.jpg"); }
  &.page-10 #page-hero {
    background-image: url("../_img/resources_hero.jpg"); }
  &.archive #page-hero {
    background-image: url("../_img/past_events_hero.jpg"); }
  &.page-12 #page-hero {
    background-image: url("../_img/our_new_center_hero.jpg"); }
  &.page-14 #page-hero {
    background-image: url("../_img/faqs_hero.jpg"); }
  &.page-8 #page-hero {
